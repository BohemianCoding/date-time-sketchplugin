{
  "name": "data-date-time",
  "version": "1.0.0",
  "description": "A data plugin that generates random dates and times",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Date & Time",
    "manifest": "src/manifest.json",
    "main": "data-date-time.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "repository": {
    "url": "https://github.com/skpm/data-date-time"
  },
  "author": "Bohemian Coding",
  "license": "MIT",
  "devDependencies": {
    "@skpm/builder": "^0.5.2"
  }
}
